<div class="navbar-form navbar-left ">
  <div class="form-group">
    <% if !user_signed_in? %>
      <%= form_tag events_path, :method => 'get', id: "zip-submit" do %>
        <%= text_field_tag(:q, "", class: "form-control", placeholder: "Zip Code") %>
      <% end %>
    <% end %>
  </div>
</div>

<div id="outside">
  <div id="content_wrapper">
    <h1>Nearby Events</h1>
    <ul class="list-group", id='eventList'>
      <% @nearby_events.each do |event| %>
        <h2><a href="/events/<%= event.id %>"><li class="list-group-item"><%= event["title"] %></li></a></h2>
      <% end %>
    </ul>
  </div>
</div>




